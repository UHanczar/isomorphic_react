{
  "name": "isomorphic_react",
  "version": "1.0.0",
  "description": "Simple isomorphic react boilerplate according to D. Stern tutorial",
  "main": "index.js",
  "repository": "https://github.com/UHanczar/isomorphic_react.git",
  "author": "U. Hanchar according to D. Stert",
  "license": "MIT",
  "scripts": {
    "build": "cross-env webpack --config ./webpack.config.prod.babel.js",
    "postinstall": "npm install -g babel-cli@6.26.0 cross-env@5.0.5 webpack@3.6.0",
    "start": "npm run start:prod",
    "start:dev": "cross-env NODE_ENV=development babel-node server --useServerRender=true --useLiveData=false",
    "start:prod": "npm run build && cross-env NODE_ENV=production babel-node server --useServerRender=true --useLiveData=true"
  },
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "bootstrap": "^4.0.0",
    "css-loader": "^0.28.10",
    "express": "^4.16.2",
    "express-yields": "^1.1.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^4.0.0",
    "lodash": "^4.17.5",
    "optimist": "^0.6.1",
    "postcss-loader": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.2.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "style-loader": "^0.20.2",
    "svg-url-loader": "^2.3.1",
    "url-loader": "^1.0.1",
    "webpack": "3.6",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "^2.21.1"
  }
}
